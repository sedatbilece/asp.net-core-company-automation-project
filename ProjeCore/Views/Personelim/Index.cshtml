
@{
    Layout = "_TestLayout";
    ViewData["Title"] = "personel görüntüle";
}


@using ProjeCore.Models
@model List<Personel>
<br />

<div>
    personelim indexi içindesiniz
    <hr />
    <br />
    <table class="table table-bordered table-active table-responsive-md">
        <tr>
            <th>ID</th>
            <th>AD</th>
            <th>SOYAD</th>
            <th>SEHİR</th>
            <th>BİRİM</th>
            <th>SİL</th>
            <th>GÜNCELLE</th>
        </tr>

        @foreach (var x in Model)
        {
    <tr>
        <td> @x.PersonelId </td>
        <td> @x.Ad</td>
        <td> @x.Soyad</td>
        <td> @x.Sehir</td>
       
        <td> @x.Birim.BirimAd</td>
        <td> ...</td>
        <td> ...</td>




    </tr>
        }

    </table>
    <a class="btn btn-primary" href="/Personelim/YeniPersonel/">yeni personel  ekle</a>


</div>
